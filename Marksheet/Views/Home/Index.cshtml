@{
    ViewData["Title"] = "Home Page";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.min.js/core@2.11.8/dist/umd/popper.min.js" ></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" ></script>
</head>
<body>
   <form onsubmit="Marksheet();" method="post" >
    <div class="container-fluid table table-bordered-10 ">
        <div class="row">
            <div class="col-sm-12 text-lg-center ">
                <h1 class="text-success">Marksheet</h1>
            </div>
        </div>
            <br />
            <br />
        <div class="row">
            <div class="col-lg-6">
            <label for="Name" class="form-label text-success ">Name</label>
            <input type="text" class="form-control " id="Name" placeholder="Student Name">
            </div>
            <div class="col-lg-6">
            <label for="Course" class="form-label text-success">Course Name:</label>
            <input type="text" class="form-control" id="Course" placeholder="Course Name">
            </div>

        </div>
            <br />
            <br />
        <div class="row">
            <div class="col-lg-6">
            <label for="Batch" class="form-label text-success">Batch Year:</label>
            <input type="number" class="form-control" id="Batch" placeholder="Enter Batch Year">
            </div>
            <div class="col-lg-6">
            <label for="Semester" class="form-label text-success" >Semester:</label>
            <select class="form-select" aria-label="Select Semester" id="Semester">
              <option selected>Select Semester</option>
              <option value="1">One</option>
              <option value="2">Two</option>
              <option value="3">Three</option>
              <option value="4">Four</option>
              <option value="5">Five</option>
              <option value="6">Six</option>
              <option value="7">Seven</option>
              <option value="8">Eight</option>
            </select>
            </div>
        </div>
            <br />
            <br />
            <div class="row">
                <div class="col-lg  col-sm  text-lg-center "><p class="text-bg-dark">SUBJECT</p></div>
                <div class="col-lg  col-sm text-lg-center"><p class="text-bg-dark" id="java1" >JAVA</p></div>
                <div class="col-lg  col-sm text-lg-center"><p class="text-bg-dark" id="html1">HTML</p></div>
                <div class="col-lg  col-sm text-lg-center"><p class="text-bg-dark" id="css1">CSS</p></div>
                <div class="col-lg  col-sm text-lg-center"><p class="text-bg-dark" id="javascrpit1">JAVASCRPIT</p></div>
                <div class="col-lg  col-sm text-lg-center"><p class="text-bg-dark" id="bootstrap1">BOOTSTRAP</p></div>

            </div>
            <br />
            <br />
            <div class="row">
                <div class="col-lg  col-sm text-lg-center"><p>SUBJECT MARKS</p></div>
                <div class="col-lg  col-sm  input-group-lg text-lg-center"><input type="number" class="form-control text-lg-center" placeholder="ENTER JAVA MARKS" id="java"></div>
                <div class="col-lg  col-sm input-group-lg text-lg-center"><input type="number" class="form-control text-lg-center" placeholder="ENTER HTML MARKS" id="html"></div>
                <div class="col-lg  col-sm input-group-lg text-lg-center"><input type="number" class="form-control text-lg-center" placeholder="ENTER CSS MARKS" id="css"></div>
                <div class="col-lg  col-sm input-group-lg text-lg-center"><input type="number" class="form-control text-lg-center" placeholder="ENTER JAVASCRPIT MARKS" id="javascrpit"></div>
                <div class="col-lg  col-sm input-group-lg text-lg-center"><input type="number" class="form-control text-lg-center" placeholder="ENTER bOOTSTRAP MARKS" id="bootstrap"></div>
            </div>
            <br />
            <br />
            <br />
            <div class="row">
                <button class="btn btn-primary col-lg-6 text-lg-center bg-primary p-1 text-center  text-white" onclick="Marksheet()" type="button" value="submit" >Submit</button>
                <button class="btn btn-primary col-lg-6 text-lg-center bg-primary p-1 text-center text-white" type="reset" value="Reset">Reset</button>   
            </div>
        
            <br />
            <br />
            <br />
            <hr class="border-4 bg-dark"/>
            <br />
            <br />
        <div>
            <table  id="tbl" class="table table-bordered-9 text-lg-center " border="1">
            <thead >
                <tr>
                 <th>Student Name</th>
                 <th>Course Name</th>
                 <th>Batch Year</th>
                 <th>Sememster</th>
                 <th>JAVA MARKS</th>
                 <th>HTML</th>
                 <th>CSS</th>
                 <th>JAVASCRPIT</th>
                 <th>BOOTSTARP</th>
                 <th>Total Marks</th>
                 <th>PERCENTAGE</th>
                 
                </tr>
            <thead>
            <tbody>


            </tbody>
            </table>

        
        </div>

    </div>
    
   </form>
   
    
    <script type="text/javascript">


        function Marksheet()
        {
            var name=document.getElementById("Name").value;
            var cname=document.getElementById("Course").value;
            var batch=document.getElementById("Batch").value;
            var sem=document.getElementById("Semester").value;
            var java=parseInt(document.getElementById("java").value);
            var html=parseInt(document.getElementById("html").value);
            var css=parseInt(document.getElementById("css").value);
            var javascrpit=parseInt(document.getElementById("javascrpit").value);
            var bootstrap=parseInt(document.getElementById("bootstrap").value);

            var TotalMarks=java+html+css+javascrpit+bootstrap;
            var PERCENTAGE= TotalMarks * 100/500;
            

            

            var tr=document.createElement('tr');

            var td1=tr.appendChild(document.createElement('td'));
            var td2=tr.appendChild(document.createElement('td'));
            var td3=tr.appendChild(document.createElement('td'));
            var td4=tr.appendChild(document.createElement('td'));
            var td5=tr.appendChild(document.createElement('td'));
            var td6=tr.appendChild(document.createElement('td'));
            var td7=tr.appendChild(document.createElement('td'));
            var td8=tr.appendChild(document.createElement('td'));
            var td9=tr.appendChild(document.createElement('td'));
            var td10=tr.appendChild(document.createElement('td'));
            var td11=tr.appendChild(document.createElement('td'));
            

            td1.innerHTML +=name;
            td2.innerHTML +=cname;
            td3.innerHTML +=batch;
            td4.innerHTML +=sem;
            td5.innerHTML +=java;
            td6.innerHTML +=html;
            td7.innerHTML +=css;
            td8.innerHTML +=javascrpit;
            td9.innerHTML +=bootstrap;
            td10.innerHTML +=TotalMarks;
            td11.innerHTML +=PERCENTAGE;
            
            

            document.getElementById("tbl").appendChild(tr);



        }

    </script>

</body>
</html>



